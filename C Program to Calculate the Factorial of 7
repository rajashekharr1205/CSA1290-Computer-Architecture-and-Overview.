#include <stdio.h>

// Function to calculate factorial
unsigned long long factorial(int n) {
    unsigned long long result = 1;
    for (int i = 1; i <= n; i++) {
        result *= i;
    }
    return result;
}

int main() {
    int number = 7;
    unsigned long long fact = factorial(number);

    printf("Factorial of %d is %llu\n", number, fact);

    return 0;
}
